@@ -3529,6 +3530,7 @@ static int __init init_ext3_fs(void)
 	err = init_inodecache();
 	if (err)
 		goto out1;
+	dynlock_cache_init();
         err = register_filesystem(&ext3_fs_type);
 	if (err)
 		goto out;
@@ -3546,6 +3548,7 @@ out1:
 static void __exit exit_ext3_fs(void)
 {
 	unregister_filesystem(&ext3_fs_type);
+	dynlock_cache_exit();
 	destroy_inodecache();
 	exit_ext3_xattr();
 	exit_ext3_proc();

